# GitLab Configuration
GITLAB_VERSION=latest
GITLAB_HOSTNAME=192.168.0.131
GITLAB_EXTERNAL_URL=http://192.168.0.131:8080
GITLAB_TIMEZONE=Asia/Tokyo
GITLAB_BACKUP_KEEP_TIME=604800
GITLAB_DB_ENCODING=unicode
GITLAB_HTTPS_ENABLED=false
GITLAB_SHM_SIZE=256m

# Port Configuration
GITLAB_HTTP_PORT=8080
GITLAB_HTTPS_PORT=8443
GITLAB_SSH_PORT=2222

# Backup Configuration
GITLAB_BACKUP_PATH=./services/gitlab/backups
BACKUP_INTERVAL=86400
BACKUP_UBUNTU_VERSION=latest

# PostgreSQL Configuration
POSTGRES_VERSION=17
POSTGRES_DB=gitlabhq_production
POSTGRES_USER=gitlab
POSTGRES_PASSWORD=your_secure_password
POSTGRES_PORT=5432

# GitLab Runner Configuration
GITLAB_RUNNER_VERSION=latest
GITLAB_RUNNER_REGISTRATION_TOKEN=xxxx
GITLAB_RUNNER_NAME=docker-runner
GITLAB_RUNNER_TAGS=docker

# GitLab Pages Configuration - namespace_in_path使用
GITLAB_PAGES_ENABLED=true
GITLAB_PAGES_EXTERNAL_URL=http://192.168.0.131:8090
GITLAB_PAGES_ACCESS_CONTROL=false

# Docker Compose Configuration
COMPOSE_PROJECT_NAME=gitlab

GITLAB_PAGES_PORT=8090